{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sin1"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "COSMIC_BUCKET_SLUG": "@cosmic_bucket_slug",
    "COSMIC_READ_KEY": "@cosmic_read_key",
    "NEXT_PUBLIC_SITE_URL": "@next_public_site_url"
  }
} 